CREATE TABLE `eventi` (
	`id` INT(11) NOT NULL AUTO_INCREMENT,
	`nome` VARCHAR(500) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`descrizione` VARCHAR(500) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`email` VARCHAR(500) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`data_scadenza` DATETIME NULL DEFAULT NULL COLLATE 'utf8_bin',
	`created_at` DATETIME NULL DEFAULT NULL COLLATE 'utf8_bin',
	`updated_at` DATETIME NULL DEFAULT NULL COLLATE 'utf8_bin',
	`attivo` INT(1) NULL DEFAULT 1 COLLATE 'utf8_bin',
	PRIMARY KEY (`id`)
)
COLLATE='utf8_bin'
ENGINE=MyISAM
AUTO_INCREMENT=2
;
